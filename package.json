{
	"name": "boldgrid-theme-framework",
	"version": "2.18.2",
	"description": "BoldGrid Theme Framework",
	"main": "index.js",
	"engines": {
		"node": ">=10.12.0"
	},
	"babel": {
		"plugins": [
			[
				"@babel/plugin-transform-runtime",
				{
					"absoluteRuntime": false,
					"corejs": 2,
					"helpers": true,
					"regenerator": true,
					"useESModules": false
				}
			],
			[
				"@babel/plugin-syntax-dynamic-import"
			]
		]
	},
	"scripts": {
		"preversion": "bash build/cwd-dirty && yarn run build",
		"postversion": "git tag -d $npm_package_version && node build/update-version.js src/boldgrid-theme-framework.php $npm_package_version && git commit -am \"Updating Version $npm_package_version\" && git tag -a $npm_package_version -m \"Version $npm_package_version Release\" master && git push origin $npm_package_version",
		"test": "echo \"Error: no test specified\" && exit 1",
		"phpcs-installStandards": "php node_modules/PHP_CodeSniffer/scripts/phpcs --config-set installed_paths node_modules/WordPress-Coding-Standards/",
		"php-codesniffer": "npm run phpcs-installStandards && php ./node_modules/PHP_CodeSniffer/scripts/phpcs -p -s -v -n ./src --standard=./phpcs.ruleset.xml --ignore=*/wp_bootstrap_navwalker.php,*/woocommerce/* --extensions=php",
		"build": "gulp build",
		"webpack-server": "cross-env NODE_ENV=development webpack-dev-server",
		"build-webpack": "webpack",
		"build-webpack-dev": "cross-env NODE_ENV=development webpack --color",
		"dev": "gulp watch",
		"dev:server": "concurrently \"cross-env NODE_ENV=development gulp watch --silent\" \"npm run webpack-server\"",
		"quick-build": "gulp qbuild",
		"script:patterns": "node --experimental-modules build/patterns.mjs",
		"script:tgm": "node build/tgm.mjs",
		"script:wp-textdomain-lint": "node build/check-text-domains.js",
		"script:zip": "node build/build-zip.js",
		"build-prime": "git clone https://github.com/boldgrid/prime ../themes/prime && ln -s $(pwd)/boldgrid-theme-framework ../themes/prime/inc/boldgrid-theme-framework"
	},
	"keywords": [
		"BoldGrid"
	],
	"author": "The BoldGrid Team",
	"license": "GPL-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/BoldGrid/boldgrid-theme-framework.git"
	},
	"resolutions": {
		"natives": "1.1.6",
		"i": "^0.3.7",
		"**/chownr": "^1.1.4",
		"**/url-parse": "^1.5.10",
		"**/follow-redirects": "^1.14.9",
		"**/nanoid": "^3.1.22"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0-beta.47",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-transform-react-jsx": "^7.3.0",
		"@babel/plugin-transform-runtime": "^7.4.4",
		"@babel/preset-env": "^7.0.0-beta.47",
		"@boldgrid/controls": "https://github.com/BoldGrid/controls#dev",
		"PHP_CodeSniffer": "https://github.com/squizlabs/PHP_CodeSniffer/#2.9",
		"WordPress-Coding-Standards": "https://github.com/WordPress-Coding-Standards/WordPress-Coding-Standards#0.10.0",
		"archiver": "^3.0.0",
		"autoprefixer": "^8.6.2",
		"babel-eslint": "^8.2.3",
		"babel-loader": "^8.0.0-beta",
		"babel-preset-minify": "^0.4.3",
		"chalk": "^2.4.2",
		"cjs": "^0.0.11",
		"concurrently": "^4.0.1",
		"cross-env": "^5.2.0",
		"css-loader": "^0.28.11",
		"cssnano": "^3.10.0",
		"del": "^2.2.0",
		"ejs-loader": "^0.3.1",
		"eslint": "^4.19.1",
		"eslint-config-wordpress": "^2.0.0",
		"eslint-loader": "^2.0.0",
		"eslint-plugin-html": "^4.0.3",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"googlefonts-sprite-generator": "https://github.com/pzagor2/GoogleFontSpriteImageGenerator",
		"gulp": "^3.9.1",
		"gulp-changed": "^1.3.0",
		"gulp-clean": "^0.3.2",
		"gulp-concat-util": "^0.5.5",
		"gulp-cssnano": "^2.1.1",
		"gulp-delete-lines": "^0.0.7",
		"gulp-imagemin": "^2.4.0",
		"gulp-inject-string": "^1.1.1",
		"gulp-jscs": "^4.0.0",
		"gulp-jshint": "^2.0.0",
		"gulp-modernizr-wezom": "^1.1.4",
		"gulp-newer": "^1.1.0",
		"gulp-phpcbf": "^0.1.2",
		"gulp-phpcs": "^1.0.0",
		"gulp-postcss": "^7.0.1",
		"gulp-rename": "^1.2.2",
		"gulp-replace": "^0.5.4",
		"gulp-sass": "^4.0.1",
		"gulp-shell": "^0.6.5",
		"gulp-sort": "^2.0.0",
		"gulp-uglify-es": "^1.0.1",
		"gulp-wp-pot": "^1.1.1",
		"gutil": "^1.6.4",
		"html-loader": "^0.5.5",
		"jshint": "^2.9.1",
		"jshint-stylish": "^2.1.0",
		"mini-css-extract-plugin": "^0.4.0",
		"mkdirp": "^0.5.1",
		"moment": "^2.29.4",
		"multislider": "https://github.com/ippontech/multiSlider.js",
		"php-parser": "^3.0.0-prerelease.8",
		"pkg-dir": "^3.0.0",
		"postcss-colors-only": "^0.0.3",
		"postcss-flexbugs-fixes": "^3.3.1",
		"postcss-hash-classname": "https://github.com/timelsass/postcss-hash-classname",
		"postcss-loader": "^2.1.5",
		"postcss-merge-rules": "^2.1.2",
		"postcss-prefix-selector": "^1.6.0",
		"run-sequence": "^1.1.5",
		"sass-loader": "^7.0.1",
		"style-loader": "^0.21.0",
		"svg-inline-loader": "^0.8.0",
		"text-table": "^0.2.0",
		"webpack": "^4.8.3",
		"webpack-cli": "^2.1.3",
		"webpack-dev-server": "^3.1.9",
		"webpack-merge": "^4.1.4",
		"webpack-runtime-public-path-plugin": "^1.1.2"
	},
	"dependencies": {
		"@babel/runtime-corejs2": "^7.13.17",
		"@boldgrid/components": "^2.16.2",
		"@wordpress/dom-ready": "^2.13.2",
		"@wordpress/hooks": "^2.12.2",
		"Buttons": "https://github.com/boldgrid/Buttons#crio-buttons",
		"animate.css": "3.7.2",
