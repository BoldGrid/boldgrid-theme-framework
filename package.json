		"color-js": "^1.0.1",
		"css-vars-ponyfill": "^1.17.2",
		"float-labels.js": "^3.3.5",
		"font-awesome": "^4.7.0",
		"hamburgers": "https://github.com/jonsuh/hamburgers#v0.9.3",
		"hero-patterns": "^2.0.2",
		"hover.css": "^2.3.1",
		"jarallax": "^1.12.4",
		"jquery-goup": "^1.1.0",
		"jquery.nicescroll": "^3.6.8",
		"kirki-toolkit": "https://github.com/kirki-framework/kirki.git#v3.1.4",
		"lodash.camelcase": "^4.3.0",
		"lodash.kebabcase": "^4.1.1",
		"lodash.lowercase": "^4.3.0",
		"lodash.startcase": "^4.4.0",
		"postcss": "^8.4.31",
		"sass.js": "~0.11.1",
		"scssphp": "https://github.com/scssphp/scssphp#v1.10.0",
		"smartmenus": "^1.1.0",
		"wow.js": "^1.2.2",
		"snyk": "1.685.0"
	},
	"snyk": true
}
